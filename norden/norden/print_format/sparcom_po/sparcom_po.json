 "html": "<meta name=\"pdfkit-orientation\" content=\"portrait\" />\r\n<meta name=\"page-size\" content ='A4'/>\r\n<header id=\"header-html\"></header>\r\n<div class=\"bigbox\">\r\n<style>\r\n@import url('https://fonts.googleapis.com/css2?family=Chonburi&display=swap');\r\n\r\n</style>\r\n<div class=\"container\">\r\n    <div class=\"row\">\r\n        <div class=\"col-xs-6\">\r\n         <img src=\"/files/SPARCOM Logo.png\" width=\"200px;\">\r\n        </div>\r\n        {% set address_line1 = frappe.db.get_value('Address',{'address_title':doc.company},'address_line1') %}\r\n        {% set address_line2 = frappe.db.get_value('Address',{'address_title':doc.company},'address_line2') %}\r\n        {% set city = frappe.db.get_value('Address',{'address_title':doc.company},'city') %}\r\n        {% set country = frappe.db.get_value('Address',{'address_title':doc.company},'country') %}\r\n        {% set phone = frappe.db.get_value('Address',{'address_title':doc.company},'phone') %}\r\n        {% set email_id = frappe.db.get_value('Address',{'address_title':doc.company},'email_id') %}\r\n        {% set web = frappe.db.get_value('Company',{'name':doc.company},'website')%}\r\n        <div class=\"divider\"></div>\r\n        <div class=\"col-xs-6\">\r\n            <!--  {% if doc.company == \"Norden Africa\" %}-->\r\n            <!--<p style=\"font-size:14px;color #ed6f1e;font-family: Verdana, sans-serif;\">Norden Communication Middle East FZE</p>-->\r\n            <!--{% endif %}-->\r\n            <!--{% if doc.company != \"Norden Africa\" %}-->\r\n        \r\n            <!--<p style=\"font-size:16px;color:#ed6f1e;font-family:Arial;font-weight:bold\">{{doc.company}}</p>-->\r\n            <!-- {% endif %}-->\r\n            <!--<p style=\"font-size:10px;\">{{address_line1 or ''}}</p>-->\r\n            <!--<p style=\"font-size:10px;\">{{address_line2 or ''}}</p>-->\r\n            <!--<p style=\"font-size:10px;\">{{city or ''}}, {{country or ''}}</p>-->\r\n            <!--<p style=\"font-size:10px;\">{% if phone %} {{phone or ''}} {% endif %}</p>-->\r\n            <!--{% if doc.company == \"Norden Communication Pvt Ltd\" %}-->\r\n            <!--<p style=\"font-size:10px;\">State Name : Kerala, Code : 32</p>-->\r\n            <!-- {% endif %}-->\r\n            <!--<p style=\"font-size:10px;\">{% if web %} Website: {{web or ''}} {% endif %}</p>-->\r\n            <!--{% if doc.company == \"Norden Singapore PTE LTD\" %}-->\r\n            <!--<p style=\"font-size:10px;\">UEN: 201811669H</p>-->\r\n            <!-- {% endif %}-->\r\n        </div>\r\n    </div>\r\n    <div class=\"line\" style=\"font-size:20px;color:#ed6f1e;text-align: center\"><b><font face=\"chonburi\"><span>&nbsp;PURCHASE ORDER</span></font></b></div>\r\n    <!--<img src=\"https://erp.nordencommunication.com/files/purcahse.jpeg\">-->\r\n    <div class='row'>\r\n        <div class=\"col-xs-7\">\r\n            <h1 style=\"font-size:14px;;font-family: Verdana, sans-serif;\">Supplier:</h1>\r\n            {% set add_line1 = frappe.db.get_value('Address',doc.supplier_address,'address_line1') %}\r\n\r\n            {% set city = frappe.db.get_value('Address',doc.supplier_address,'city') %}\r\n            {% set country = frappe.db.get_value('Address',doc.supplier_address,'country') %}\r\n\r\n            <p style=\"font-size:10px;\">{{doc.supplier}}</p>\r\n\r\n            <p style=\"font-size:10px;\">{{add_line1 or ''}}</p>\r\n            <p style=\"font-size:10px;\">{{add_line2 or ''}}</p>\r\n            <p style=\"font-size:10px;\">{{city or ''}}</p>\r\n            <p style=\"font-size:10px;\">{{country or ''}}</p>\r\n\r\n            {% if doc.ship_to %}\r\n            <h2 style=\"font-size:14px;font-family: Verdana, sans-serif;\">Ship to:</h2>\r\n            {% set address_title = frappe.db.get_value('Address',doc.shipping_address,'address_title') %}\r\n            {% set add_line1 = frappe.db.get_value('Address',doc.shipping_address,'address_line1') %}\r\n            {% set add_line2 = frappe.db.get_value('Address',doc.shipping_address,'address_line2') %}\r\n            {% set city = frappe.db.get_value('Address',doc.shipping_address,'city') %}\r\n            {% set country = frappe.db.get_value('Address',doc.shipping_address,'country') %}\r\n\r\n            <p style=\"font-size:10px;\">{{address_title or ''}}</p>\r\n            <p style=\"font-size:10px;\">{{add_line1 or ''}}</p>\r\n            <p style=\"font-size:10px;\">{{add_line2 or ''}}</p>\r\n            <p style=\"font-size:10px;\">{{city or ''}}</p>\r\n            <p style=\"font-size:10px;\">{{country or ''}}</p>\r\n            {% endif %}\r\n        </div>\r\n        <div class=\"col-xs-5\">\r\n            <div class='red-box' style=\"width:250px;text-align:center\" nowrap>{{doc.name[0:]}}</div>\r\n            <table class=\"table table-bordered\">\r\n                 <tr>\r\n                    <td style=\"border-color:#ed6f1e\">Order Date</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{doc.get_formatted(\"transaction_date\")}}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td style=\"border-color:#ed6f1e\">Currency</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{doc.currency}}</td>\r\n                </tr>\r\n               \r\n                <tr>\r\n                    <td style=\"border-color:#ed6f1e\">Required Date</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{doc.get_formatted(\"schedule_date\")}}</td>\r\n                </tr>\r\n                \r\n                 <tr>\r\n                    <td style=\"border-color:#ed6f1e\">Supplier Ref</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{doc.ref_supp or ''}}</td>\r\n                </tr>\r\n                <!-- <tr>-->\r\n                <!--    <td style=\"border-color:#ed6f1e\">Our TRN #</td>-->\r\n                <!--    <td style=\"border-color:#ed6f1e\">{{doc.our_trn or ''}}</td>-->\r\n                <!--</tr>-->\r\n                 <tr>\r\n                    <td style=\"border-color:#ed6f1e\">Delivery Terms</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{doc.delivery_term or ''}}&nbsp;&nbsp;{{doc.loc or''}}</td>\r\n                </tr>\r\n                \r\n                <tr>\r\n                    <td style=\"border-color:#ed6f1e\">Batch No</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{doc.batch}}</td>\r\n                </tr>\r\n            </table>\r\n        </div>\r\n    </div>\r\n\r\n    {% if doc.supplier_part_no and doc.mrp %}\r\n  \r\n\r\n    <div class=\"row\">\r\n        <div class=\"col-xs-12\">\r\n            <table class=\"table table-bordered\">\r\n                <thead>\r\n                <tr>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">No\r\n                    </td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\" width=\"24%\">\r\n                        Item</td>\r\n                    <!--<td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">-->\r\n                    <!--    Supplier Part No.</td>-->\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\" >\r\n                        Description</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        QTY</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        UOM</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        Rate</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        Amount</td>\r\n                    <td style=\"border-color:red;border-right-color:#ed6f1e;background-color: #ed6f1e;color: white;\">MRP</td>\r\n                \r\n                 <!--<td style=\"border-color:red;border-right-color:red;background-color: #ed6f1e;color: white;\">Remarks</td>-->\r\n                </tr>\r\n                </thead>\r\n                {% for row in doc.items %}\r\n                <tr>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.idx}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.item_code}}<br><br>{{row.supplier_part_no or \"\"}}</td>\r\n                    <!--<td style=\"border-color:#ed6f1e\">{{row.supplier_part_no}}</td>-->\r\n                    <td style=\"border-color:#ed6f1e\">{{row.description}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.qty}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.uom}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(row.rate)}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(row.amount)}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(row.mrp)}}</td>\r\n                    <!--<td style=\"border-color:#ed6f1e\">{{row.remark or ''}}</td>-->\r\n\r\n                        {% endfor %}\r\n                </tr>\r\n                {% if doc.discount_amount %}\r\n                 <tr>\r\n                    <td colspan=\"7\" style=\"border-color:#ed6f1e\">Total Discount</td>\r\n                    <td colspan=\"1\" style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(doc.discount_amount)}}</td>\r\n                </tr>\r\n                {% endif %}\r\n                        {% for row in doc.taxes %}\r\n\r\n                <tr>\r\n                    <td colspan=\"7\" style=\"border-color:#ed6f1e\">{{row.account_head}}</td>\r\n                    <td colspan=\"1\" style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(row.tax_amount)}}</td>\r\n                </tr>\r\n                {% endfor %}\r\n                <tr>\r\n                    <td colspan=\"7\" style=\"border-color:#ed6f1e;text-align:right\">Net Total</td>\r\n                    <td  colspan=\"1\" style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(doc.grand_total)}}</td>\r\n                </tr>\r\n                <!--<tr>-->\r\n                <!--    <td colspan=\"8\" style=\"border-color:#ed6f1e;background-color: lightgrey;text-align:right\">-->\r\n                <!--        {{doc.in_words}}</td>-->\r\n                <!--</tr>-->\r\n            </table>\r\n        </div>\r\n    </div>\r\n\r\n\r\n    {% elif doc.supplier_part_no %}\r\n\r\n    <div class=\"row\">\r\n        <div class=\"col-xs-12\">\r\n            <table class=\"table table-bordered\">\r\n                <thead>\r\n                <tr>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">No\r\n                    </td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\" width=\"24%\">\r\n                        Item</td>\r\n                    <!--<td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">-->\r\n                    <!--    Supplier Part No.</td>-->\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        Description</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        QTY</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        UOM</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        Rate</td>\r\n                     <td style=\"border-color:red;border-right-color:#ed6f1e;background-color: #ed6f1e;color: white;\">Amount</td>\r\n                \r\n                 <!--<td style=\"border-color:red;border-right-color:red;background-color: #ed6f1e;color: white;\">Remarks</td>-->\r\n                </tr>\r\n                </thead>\r\n                {% for row in doc.items %}\r\n                <tr>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.idx}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.item_code}}<br><br>{{row.supplier_part_no or \"\"}}</td>\r\n                    <!--<td style=\"border-color:#ed6f1e\">{{ row.supplier_part_no or ''}}</td>-->\r\n                    <td style=\"border-color:#ed6f1e\">{{row.description}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.qty}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.uom}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(row.rate)}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(row.amount)}}\r\n                    <!--<td style=\"border-color:#ed6f1e\">{{row.remark or ''}}</td>-->\r\n                </tr>\r\n                {% endfor %}\r\n                {% if doc.discount_amount %}\r\n                 <tr>\r\n                    <td colspan=\"6\" style=\"border-color:#ed6f1e\">Total Discount</td>\r\n                    <td colspan=\"1\" style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(doc.discount_amount)}}</td>\r\n                </tr>\r\n                {% endif %}\r\n                 {% for row in doc.taxes %}\r\n\r\n                <tr>\r\n                    <td colspan=\"6\" style=\"border-color:#ed6f1e\">{{row.account_head}}</td>\r\n                    <td colspan=\"1\" style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(row.tax_amount)}}</td>\r\n                </tr>\r\n                {% endfor %}\r\n                <tr>\r\n                    <td colspan=\"6\" style=\"border-color:#ed6f1e;text-align:right\">Net Total</td>\r\n                    <td colspan=\"1\" style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(doc.grand_total)}}</td>\r\n                </tr>\r\n                <!--<tr>-->\r\n                <!--    <td colspan=\"8\" style=\"border-color:#ed6f1e;background-color: lightgrey;text-align:right\">-->\r\n                <!--        {{doc.in_words}}</td>-->\r\n                <!--</tr>-->\r\n            </table>\r\n        </div>\r\n    </div>\r\n    {% elif doc.mrp %}\r\n    <div class=\"row\">\r\n        <div class=\"col-xs-12\">\r\n            <table class=\"table table-bordered\">\r\n                <thead>\r\n                <tr>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">No\r\n                    </td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\" width=\"24%\">\r\n                        Item</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        Description</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        QTY</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        UOM</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        Rate</td>\r\n                    <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">\r\n                        Amount</td>\r\n                     <td style=\"border-color:red;border-right-color:#ed6f1e;background-color: #ed6f1e;color: white;\">MRP</td>\r\n                \r\n                 <!--<td style=\"border-color:red;border-right-color:red;background-color: #ed6f1e;color: white;\">Remarks</td>-->\r\n                </tr>\r\n                </thead>\r\n                {% for row in doc.items %}\r\n                <tr>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.idx}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.item_code}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.description}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.qty}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{row.uom}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(row.rate)}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(row.amount)}}</td>\r\n                    <td style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(row.mrp)}}\r\n                    <!--<td style=\"border-color:#ed6f1e\">{{row.remark or ''}}</td>-->\r\n                </tr>\r\n                {% endfor %}\r\n                {% if doc.discount_amount %}\r\n                 <tr>\r\n                    <td colspan=\"7\" style=\"border-color:#ed6f1e\">Total Discount</td>\r\n                    <td colspan=\"1\" style=\"border-color:#ed6f1e\">{{\"{:,.2f}\".format(doc.discount_amount)}}</td>\r\n                </tr>\r\n                {% endif %}\r\n                 {% for row in doc.taxes %}\r\n\r\n                <tr>\r\n                    <td colspan=\"7\" style=\"border-color:#ed6f1e\">{{row.account_head}}</td>\r\n                    <td colspan=\"1\" style=\"border-color:#ed6f1e\">{{row.tax_amount}}</td>\r\n                </tr>\r\n                {% endfor %}\r\n                <tr>\r\n                    <td colspan=\"7\" style=\"border-color:#ed6f1e;text-align:right\">Net T