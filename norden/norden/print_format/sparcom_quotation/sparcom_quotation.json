 "html": "<div class=\"bigbox\">\n<style>\n@import url('https://fonts.googleapis.com/css2?family=Chonburi&display=swap');\n</style>\n<div class=\"container\">\n    <div class=\"row\">\n        <div class=\"col-xs-6\">\n            <br>\n            <img src=\"/files/SPARCOM Logo.png\" width=\"200px\">\n        </div>\n        <!--{% set address_line1 = frappe.db.get_value('Address',{'address_title':doc.company},'address_line1') %}-->\n        <!--{% set address_line2 = frappe.db.get_value('Address',{'address_title':doc.company},'address_line2') %}-->\n        <!--{% set city = frappe.db.get_value('Address',{'address_title':doc.company},'city') %}-->\n        <!--{% set country = frappe.db.get_value('Address',{'address_title':doc.company},'country') %}-->\n        <!--{% set phone = frappe.db.get_value('Address',{'address_title':doc.company},'phone') %}-->\n        <!--{% set email_id = frappe.db.get_value('Address',{'address_title':doc.company},'email_id') %}-->\n        <!--{% set pincode = frappe.db.get_value('Address',{'address_title':doc.company},'pincode') %}-->\n        <!--{% set tax_id = frappe.db.get_value('Company',{'name':doc.company},'tax_id') %}-->\n        <!--<div class=\"divider\"></div>-->\n        <!--<div class=\"divider\"></div>-->\n        <!--<div class=\"col-xs-6\"><br>-->\n        <!--    {% if doc.company == \"Norden Africa\" %}-->\n        <!--    <p style=\"font-size:16px;color:#ed6f1e;font-family:Arial;font-weight:bold\">Norden Communication Middle East FZE</p>-->\n        <!--    {% endif %}-->\n        <!--    {% if doc.company != \"Norden Africa\" %}-->\n        <!--    <p style=\"font-size:16px;color:#ed6f1e;font-family:Arial;font-weight:bold\">{{doc.company or ''}}</p>-->\n        <!--    {% endif %}-->\n        <!--    <p style=\"font-size:10px;\">{{address_line1 or ''}}</p>-->\n        <!--    <p style=\"font-size:10px;\">{{address_line2 or ''}}</p>-->\n        <!--    <p style=\"font-size:10px;\">{{city}}</p>-->\n        <!--     {% if phone %}-->\n        <!--    <p style=\"font-size:10px;\">Phone:{{phone or ''}}</p>-->\n        <!--    {% endif %}-->\n        <!--     {% if email_id %}-->\n        <!--    <p style=\"font-size:10px;\">Email:{{email_id or ''}}</p>-->\n        <!--     {% endif %}-->\n        <!--    {% if doc.company == \"Norden Communication Pvt Ltd\" %}-->\n        <!--    <p style=\"font-size:10px;\">State Name : Kerala, Code : 32</p>-->\n        <!--     {% endif %}-->\n        <!--    {% if tax_id%}-->\n        <!--    <p style=\"font-size:12px;\"><b>VAT: {{tax_id or ''}}</b></p>-->\n        <!--    {% endif %}-->\n        <!--    {% if doc.company == \"Norden Singapore PTE LTD\"%}-->\n        <!--    <p style=\"font-size:10px;\">UEN: 201811669H</p>-->\n        <!--    {% endif %}-->\n        <!--</div>-->\n    </div>\n\n    <div class=\"line\" style=\"font-size:20px;color:#ed6f1e;\"><b><font face=\"chonburi\"><span >QUOTATION</span></font></b></div>\n    <!--<p style=\"font-size:10px;\"><b>REF:</b>{{doc.project_name or ''}}</p>-->\n    <!--<p style=\"font-size:10px;\"><b>DATE:</b>{{doc.transaction_date}}</p>-->\n    \n    <div class='row'>\n        <div class=\"col-xs-6\">\n            <!--<p style=\"font-size:11px;\"><b>Customer Reference:&nbsp;</b>{{doc.project_reference or ''}}</p>-->\n            <p style=\"font-size:11px;\"><b>Enquiry Date:&nbsp;</b>{{frappe.utils.formatdate(doc.enquiry_date or '')}}</p>\n            <h3 style=\"font-size:11px;font-family: Verdana, sans-serif;\">Quotation To:</h3>\n            {% set add_line1 = frappe.db.get_value('Address',doc.supplier_address,'address_line1') %}\n            {% set add_line2 = frappe.db.get_value('Address',doc.supplier_address,'address_line2') %}\n            {% set city = frappe.db.get_value('Address',doc.supplier_address,'city') %}\n            {% set country = frappe.db.get_value('Address',doc.supplier_address,'country') %}\n            {% set vat = frappe.db.get_value('Customer',{'name':doc.party_name},'tax_id') %}\n             <p style=\"font-size:10px;\"><b>{{doc.title}}</b></p>\n            <p style=\"font-size:10px;\">{{doc.address_display}}</p>\n            {% if vat %}\n            <p style=\"font-size:10px;\">VAT :{{vat}}</p>\n            {% endif %}\n            <!--<p style=\"font-size:10px;\">{{doc.supplier_address or ''}}</p>-->\n            <!--<p style=\"font-size:10px;\">{{add_line1 or ''}}</p>-->\n            <!--<p style=\"font-size:10px;\">{{add_line2 or ''}}</p>-->\n            <!--<p style=\"font-size:10px;\">{{city or ''}}</p>-->\n            <!--<p style=\"font-size:10px;\">{{country or ''}}</p><br><br>-->\n            {% if doc.same_as %}\n            <h3 style=\"font-size:11px;font-family: Verdana, sans-serif;\">Ship to:</h3>\n              <p style=\"font-size:10px;\"><b>{{doc.title}}</b></p>\n            <p style=\"font-size:10px;\">{{doc.address_display}}</p>\n            {% else %}\n            {% if doc.shipping_address_text %}\n            <h3 style=\"font-size:11px;font-family: Verdana, sans-serif;\">Ship to:</h3>\n            <p style=\"font-size:10px;\">{{doc.shipping_address_text}}</p>\n            <!--{% set add_line1 = frappe.db.get_value('Address',doc.shipping_address_name,'address_line1') %}-->\n            <!--{% set add_line2 = frappe.db.get_value('Address',doc.shipping_address_name,'address_line2') %}-->\n            <!--{% set city = frappe.db.get_value('Address',doc.shipping_address_name,'city') %}-->\n            <!--{% set country = frappe.db.get_value('Address',doc.shipping_address_name,'country') %}-->\n            \n           \n            <!--<p style=\"font-size:11px;\">{{doc.shipping_address_name or ''}}</p>-->\n            <!--<p style=\"font-size:11px;\">{{add_line1 or ''}}</p>-->\n            <!--<p style=\"font-size:11px;\">{{add_line2 or ''}}</p>-->\n            <!--<p style=\"font-size:11px;\">{{city or ''}}</p>-->\n            <!--<p style=\"font-size:11px;\">{{country or ''}}</p>-->\n            {% endif %}\n             {% endif %}\n           <p style=\"font-size:11px;\"><b>Kind ATT:&nbsp; </b>{{doc.contact_display or ''}}</p>\n           <p style=\"font-size:11px;\"><b>SUB: &nbsp;</b>{{doc.project_name or ''}}</p><br>\n            \n           \n        </div>\n         \n        <div class=\"col-xs-6\">\n            {% if doc.company ==\"Norden Communication Pvt Ltd\" %}\n             <div class='red-box'>{{doc.name[0:]}}</div>\n            {% elif doc.company ==\"Norden Communication Middle East FZE\" %}\n             <div class='red-box'>{{doc.name[0:]}}</div>\n            {% elif doc.company ==\"NCPL -Bangalore\" %}\n             <div class='red-box'>{{doc.name[0:]}}</div>\n            {% elif doc.company == \"Norden Communication India\" %}\n             <div class='red-box'>{{doc.name[0:]}}</div>\n            {% elif doc.company == \"Sparcom Ningbo Telecom Ltd\" %}\n             <div class='red-box' >{{doc.name[0:]}}</div>\n            {% elif doc.company == \"Norden Communication UK Limited\" %}\n             <div class='red-box'>{{doc.name[0:]}}</div>\n            {% elif doc.company == \"Norden Singapore PTE LTD\" %}\n             <div class='red-box'>{{doc.name[0:]}}</div>\n             {% elif doc.company == \"Norden Africa\" %}\n             <div class='red-box'>{{doc.name[0:]}}</div>\n            \n            {% endif %}\n        <table class=\"table table-bordered\" style=\"font-size:10px;\">\n            <tr><td style=\"border-color:#ed6f1e\">File No</td><td Colspan=\"2\" style=\"border-color:#ed6f1e\">{{doc.file_number or ''}}</td></tr>\n            <tr><td style=\"border-color:#ed6f1e\">Quote Date</td><td Colspan=\"2\" style=\"border-color:#ed6f1e\">{{doc.get_formatted(\"transaction_date\")}}</td></tr>\n            <tr><td style=\"border-color:#ed6f1e\">Currency</td><td Colspan=\"2\" style=\"border-color:#ed6f1e\">{{doc.currency}}</td></tr>\n            \n            <tr><td style=\"border-color:#ed6f1e\">Sales Person</td><td Colspan=\"2\" style=\"border-color:#ed6f1e\">{{doc.sales_person_name or ''}}</td></tr>\n            <tr><td style=\"border-color:#ed6f1e\">Validity</td><td  Colspan=\"2\" style=\"border-color:#ed6f1e\">{{doc.get_formatted(\"valid_till\")}}</td></tr>\n            <tr><td style=\"border-color:#ed6f1e\">Our REF </td ><td Colspan=\"2\" style=\"border-color:#ed6f1e\">{{doc.opportunity or''}}</td></tr>\n            <tr><td style=\"border-color:#ed6f1e\">Delivery Terms</td ><td Colspan=\"2\" style=\"border-color:#ed6f1e\">{{doc.delivery_terms or ''}}&nbsp;&nbsp;&nbsp;{{doc.country or ''}}</td></tr>\n            </table>\n        </div>\n       \n    </div>\n    <div class=\"row\">\n    <div class=\"col-xs-12\">\n\n     <p style=\"font-size:11px;\"> Dear Sir/Madam,</p>\n     <p style=\"font-size:11px;font-family:Arial;\">Thank you for your enquiry.We are pleased to quote you the following:</p>\n     <table id=\"tablesize\" class=\"table table-bordered\">\n                <thead class=\"head\">\n                    {% if doc.company == \"Sparcom Ningbo Telecom Ltd\" %}\n                <tr style=\"height:10%;\"><td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">No</td>\n                <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;width:18%;\">Item</td>\n                <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;width:40%,height:10px;\">Description</td>\n                <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">UOM</td>\n                <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">Qty</td>\n                <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">Rate </td>\n                <td style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">Amount</td>\n                <td style=\"border-color:#ed6f1e;border-right-color:#ed6f1e;background-color: #ed6f1e;color: white;\">Remarks</td> </tr>\n                </thead>\n                \n                \n                {% for row in doc.items %}\n                {% if row.optional_item %}\n                <tr class =\"altercolor\" style=\"font-size:10px; height:10%;\"><td style=\"font-family:Arial;border-color:#ed6f1e;background-color: #F1EAE8;color: black;\">{{row.idx}}</td><td style=\"border-color:#ed6f1e;background-color: #F1EAE8;color: black;\"> <a href=\"{{row.product_link}}\" target=\"_blank\" rel=\"noreferrer\" >{{row.item_code}}</a> </td><td style=\"border-color:#ed6f1e;background-color: #F1EAE8;color: black;\">{{row.description}}</td><td style=\"border-color:#ed6f1e;background-color: #F1EAE8;color: black;\">{{row.uom}}</td><td style=\"border-color:#ed6f1e;background-color: #F1EAE8;color:black;\">{{(row.qty)|int}}</td><td style=\"border-color:#ed6f1e;background-color: #F1EAE8;color: black;\"><span class=\"currency\">{{\"{:,.2f}\".format( row.rate)}}</span></td>\n                <td style=\"border-color:#ed6f1e;background-color: #F1EAE8;color: black;\"><span class=\"currency\">{{\"{:,.2f}\".format(row.amount)}}</span></td>\n                \n                <td style=\"border-color:#ed6f1e;background-color: #F1EAE8;color: black;\">{{row.remark or ''}}</td></tr>\n                \n                {% else %}\n                <tr class =\"altercolor\" style=\"font-size:10px; height:10%;\"><td style=\"font-family:Arial;border-color:#ed6f1e;\">{{row.idx}}</td><td style=\"border-color:#ed6f1e\"><a href=\"{{row.product_link}}\" target=\"_blank\" rel=\"noreferrer\" >{{row.item_code}}</a></td><td style=\"border-color:#ed6f1e\">{{row.description}}</td><td style=\"border-color:#ed6f1e\">{{row.uom}}</td><td style=\"border-color:#ed6f1e\">{{(row.qty)|int}}</td><td style=\"border-color:#ed6f1e\"><span class=\"currency\">{{\"{:,.2f}\".format( row.rate)}}</span></td><td style=\"border-color:#ed6f1e\"><span class=\"currency\">{{\"{:,.2f}\".format(row.amount)}}</span></td><td style=\"border-color:#ed6f1e\">{{row.remark or ''}}</td></tr>\n                {% endif %}\n                {% endfor %}\n                {% if doc.quotation_optional_item %}\n                 <tr style=\"height:10%;\"><td colspan=\"8\" style=\"border-color:#ed6f1e;border-right-color:white;background-color: #ed6f1e;color: white;\">OPTIONAL ITEMS</td></tr>\n                {% for row in doc.quotation_optional_item %}\n              \n                <tr class =\"altercolor\" style=\"font-size:10px; height:10%;\"><td style=\"font-family:Arial;border-color:#ed6f1e;\">{{row.idx}}</td><td style=\"border-color:#ed6f1e\">{{row.item_code}}</td><td style=\"border-color:##ed6f1e\">{{row.description}}</td><td style=\"border-color:##ed6f1e\">{{row.uom}}</td><td style=\"border-color:#ed6f1e\">{{(row.qty)|int}}</td><td style=\"border-color:#ed6f1e\"><span class=\"currency\">{{\"{:,.2f}\".format(row.rate)}}</span></td><td style=\"border-color:#ed6f1e\"><span class=\"currency\">{{\"{:,.2f}\".format(row.amount)}}</span></td><td style=\"border-color:#ed6f1e\">{{row.remark or ''}}</td></tr>\n                {% endfor %}\n              \n                {% endif %}\n               \n                \n                <tr><td colspan=\"6\" style=\"border-color:#ed6f1e;text-align:right;font-size:11px;\" >Total</td><td class=\"myDIV\" colspan=\"2\" style=\"border-color:#ed6f1e;font-size:11px;\"  pattern=\"^\\d*(\\.\\d{0,4})?$\"><span >{{\"{:,.2f}\".format(doc.total)}}</span></td></tr>\n                {% if doc.discount_amount %}\n                <tr><td colspan=\"7\" style=\"border-color:#ed6f1e;text-align:right;font-size:11px;\" >Discount</td><td class=\"myDIV\" colspan=\"2\" style=\"border-color:#ed6f1e;font-size:11px;\"  pattern=\"^\\d*(\\.\\d{0,4})?$\"><span >{{\"{:,.2f}\".format(doc.discount_amount)}}</span></td></tr>\n                {% endif %}\n               \n                {% for row in doc.taxes %}\n                {% if doc.company==\"Norden Singapore PTE LTD-SAARC\" or doc.company==\"Norden Communication Pvt Ltd\"%}\n                <tr><td colspan=\"7\" style=\"border-color:##ed6f1e;font-size:11px;text-align:right\">{{row.account_head[:-7]}}</td><td  colspan=\"2\" style=\"border-color:#ed6f1e;font-size:11px;\"><span >{{ \"{:,.2f}\".format(row.base_tax_amount)}}</span></td></tr>\n                {% elif doc.company==\"Norden Singapore PTE LTD\" or doc.company==\"Norden Communication UK Limited\" or  doc.company==\"Norden Communication Middle East FZE\" %}\n                <tr><td colspan=\"7\" style=\"border-color:#ed6f1e;font-size:11px;text-align:right\">{{row.account_head[:-6]}}</td><td  colspan=\"2\" style=\"border-color:#ed6f1e;font-size:11px;\"><span >{{ \"{:,.2f}\".format(doc.total_taxes_and_charges)}}</span></td></tr>\n                {% elif doc.company==\"Norden Africa\"%}\n                 <tr><td colspan=\"7\" style=\"border-color:##ed6f1e;font-size:11px;text-align:right\">{{row.account_head}}</td><td  colspan=\"2\" style=\"border-color:#ed6f1e;font-size:11px;\"><span >{{ \"{:,.2f}\".format(row.base_tax_amount)}}</span></td></tr>\n                \n                {% elif doc.company==\"Norden Communication India\"%}\n                 <tr><td colspan=\"7\" style=\"border-color:#ed6f1e;font-size:11px;text-align:right\">{{row.account_head[:-4]}}</td><td  colspan=\"2\" style=\"border-color:#ed6f1e;font-size:11px;\"><span >{{ \"{:,.2f}\".format(row.base_tax_amount)}}</span></td></tr>\n                {% endif %}\n                {% endfor %}\n                {% if doc.round_off %}\n                <tr><td colspan=\"6\" style=\"border-color:#ed6f1e;text-align:right;font-size:11px;\" >Net Total</td><td class=\"myDIV\" colspan=\"2\" style=\"border-color:#ed6f1e;font-size:11px;\"  pattern=\"^\\d*(\\.\\d{0,4})?$\"><span >{{ \"{:,.2f}\".format(doc.rounded_total) }}</span></td></tr>\n                {% else %}\n                <tr><td colspan=\"6\" style=\"border-color:#ed6f1e;text-align:right;font-size:11px;\" >Net Total</td><td class=\"myDIV\" colspan=\"2\" style=\"border-color:#ed6f1e;font-size:11px;\"  pattern=\"^\\d*(\\.\\d{0,4})?$\"><span >{{ \"{:,.2f}\".format(doc.grand_total) }}</span></td></tr>\n                {% endif %}\n                <tr><td colspan=\"9\" style=\"border-color:#ed6f1e;background-color:\t#A9A9A9;font-size:11px;text-align:right\">{{doc.in_words}}</td></tr>\n                  {% endif %}\n            </table>\n        </div>\n    </div><br><br>\n    <p style=\"font-size:11px;font-family:Arial;\">** Kindly note that the above price will be valid till {{doc.valid_till}} only, so please confirm your order before that**&nbsp;&nbsp;<b>Production lead time: &nbsp;</b> {{doc.production_lead_time or ''}}</p>\n    \n    </div>\n     \n{% if doc.terms %}   \n\n<H4 style=\" color:#ed6f1e\"><br><br> Terms and Conditions</h4>\n\n<p  style=\"font-size:12px;\">{{doc.terms}}</p>\n{% endif %}\n\n {% if doc.print == 1%}\n    <p style=\"page-break-before: always;\">&nbsp;</p>\n    {% endif %}\n    \n{% if doc.payment_terms_template %}\n<div class=\"row\">\n<div class=\"col-xs-12\">\n    <H5 style=\"color:#ed6f1e;\">Payment Terms</h5>\n<p style=\"font-size:10px;\">{{doc.payment_terms_template}}</p>\n <!--<table class=\"table table-bordered\">-->\n <!--               <thead>-->\n <!--               <tr><td style=\"border-color:#e20026;border-right-color:white;background-color: #e20026;color: white;\">No</td><td style=\"border-color:#e20026;border-right-color:white;background-color: #e20026;color: white;\">Payment Term</td><td style=\"border-color:#e20026;border-right-color:white;background-color: #e20026;color: white;\">Description</td><td style=\"border-color:#e20026;border-right-color:white;background-color: #e20026;color: white;\">Due Date</td><td style=\"border-color:#e20026;border-right-color:white;background-color: #e20026;color: white;\">Invoice Portion</td><td style=\"border-color:#e20026;border-right-color:#e20026;background-color: #e20026;color: white;\"> Payment Amount</td> </tr>-->\n <!--                </thead>-->\n <!--                {% for row in doc.payment_schedule %}-->\n <!--                <tr style=\"font-size:10px\"><td style=\"border-color:#e20026\">{{row.idx}}</td><td style=\"border-color:#e20026\">{{row.payment_term}}</td><td style=\"border-color:#e20026\">{{row.description}}</td><td style=\"border-color:#e20026\">{{row.due_date}}</td><td style=\"border-color:#e20026\">{{row.invoice_portion|int }}%</td><td style=\"border-color:#e20026\">{{ \"{:,.2f}\".format(row.payment_amount)}}</td></tr>-->\n <!--                {% endfor %}-->\n <!--                </table>-->\n                 </div>\n                 </div>\n{% endif %}\n{% if doc.warranty_print == 1%}\n    <p style=\"page-break-before: always;\">&nbsp;</p>\n    {% endif %}\n{% if doc.warranty_deatails %}<br>\n<h4  style=\" color:#ed6f1e\">Warranty Terms</h4>\n<p style=\"font-size:12px;\">{{doc.warranty_deatails}}</p><br>\n{% endif %}\n<br><br><h5>&nbsp;For</h5>\n<h6>&nbsp;{{doc.company or ''}}</h6>\n<h6>&nbsp;{{doc.prepared_name or''}}</h6>\n{% set letter_head =frappe.get_doc(\"Letter Head\",\"footer\") %}\n <div class=\"row\">\n    <div class =\"col-xs-12\">\n     {% if doc.compliance_sheet %}\n        <a href=\"{{doc.compliance_sheet}}\">&nbsp;Click here for compliance sheet</a>\n    {% endif %}\n    <p style=\"font-size:12px;\">&nbsp;Thank you for having business with us!</p>\n    <!--<h2 id=\"footer-html\" width=\"100%\">{{letter_head.footer}}</h2>-->\n    <div class=\"col-xs-14\" ><br><br>\n    <!--<img src=\"/files/SPARCOM Address.png\" width=\"100%\"><br><br>{{letter_head.footer}}-->\n     <h6 id=\"footer-html\" width=\"100%\"><img src=\"/files/SPARCOM Address.png\" width=\"100%\"><br><br>{{letter_head.footer}}</h6></div>\n</div>\n</div>\n</div>",
