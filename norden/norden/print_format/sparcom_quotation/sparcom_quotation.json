 "css": ".divider:before,\n.divider:after {\n    content: \"\";\n    flex: 1;\n    border-bottom: 1px solid orange;\n    margin:auto;\n}\n/*.bigbox{*/\n/*    margin:20px;*/\n/*}*/\n\n.red-box {\n  background-color: #ed6f1e;\n  content:justify;\n  width: 306px;\n  padding: 5px;\n  color: white;\n}\n.print-format{\n   margin-top:3px;\n   margin-bottom:3px;\n    margin-left:3px;\n    margin-right:3px;\n}\n\n\n\n/*.termsize{*/\n/*    font-size:12px;*/\n/*}*/\n\n/*.watermarked {*/\n/*  position: relative;*/\n/*}*/\n/*.watermarked:after {*/\n/*  content: \"\";*/\n/*  display: block;*/\n/*  width: 100%;*/\n/*  height: 100%;*/\n/*  position: absolute;*/\n/*  top: 0px;*/\n/*  left: 0px;*/\n/*  background-image: url(\"http://placehold.it/100x100/09f/fff.png\");*/\n/*  background-size: 100px 100px;*/\n/*  background-position: 30px 30px;*/\n/*  background-repeat: no-repeat;*/\n/*  opacity: 0.7;*/\n/*}*/\n/*.container{*/\n/*     background-color: #e20026;*/\n/*}*/\n.line {\n  position: relative;\n  padding-left:33rem;\n}\n.line:after {\n  content: \"\";\n  border-bottom: 2.5px solid #ed6f1e;\n  position: absolute;\n  color:#ed6f1e;\n  top: 50%;\n  left: 0;\n  right: 0;\n  width: 100%;\n  z-index: -1;\n}\n.line span {\n  padding: 0 8px;\n  background-color: white;\n  color:#FF5F1F;\n  position: relative;\n}\n\n\n.altercolor:nth-child(odd){ \n\t\tbackground-color:#ed6f1e  ;\n\t}\n\n#background{\n position:relative;\n bottom:5px;\n right:10px;\n top:10px;\n opacity:0.2;\n z-index:10;\n color:white;\n}\n/*td{*/\n    /*white-space:nowrap;*/\n    /*width:100%;*/\n/*}*/\n\n/*#bg-text*/\n/*{*/\n/*    color:lightgrey;*/\n/*    font-size:100px;*/\n/*    transform:rotate(300deg);*/\n/*    -webkit-transform:rotate(300deg);*/\n/*}*/\n/*footer {*/\n/*    display: flex;*/\n/*    justify-content: center;*/\n/*    padding: 5px;*/\n/*    background-color: #e20026;*/\n/*    color: #fff;*/\n/*}*/\n",
