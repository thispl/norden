 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\" />\r\n<!--<style>-->\r\n<!--table, th, td {-->\r\n<!--  border: 1px solid black;-->\r\n  <!--/*border-collapse: collapse;*/-->\r\n  <!--/*width=200;*/-->\r\n  <!--/*<div id=\"mainHolder\" style=\"overflow: auto; max-height: 500px;\">*/-->\r\n<!--}-->\r\n<!--</style>-->\r\n    <table class =\"table table-bordered\"  style = \"width:100%;font-size:10px;height:auto\">\r\n    <tr>\r\n      <td colspan=\"20\"    style = \"width:15%;font-size:5px;\"><img src=\"/files/Norden Logo.png\"></td>\r\n      <td colspan ='160'  width='170' style ='text-align:center;font-size:20px'><b>{{doc.company}}</b></td>\r\n    </tr> \r\n    \r\n    <tr>\r\n      <td  colspan ='150'  width=\"160\" style ='text-align: center;font-size:15px'> <b> GOODS RECEIVING REPORT(G.R.R)</b> </td>\r\n      <td  colspan ='50'   style ='text-align:right;font-size:15px'>Date : {{doc.posting_date}}</td>\r\n    </tr>\r\n      \r\n      <tr  class=\"no-bottom-border\">\r\n      <td  colspan ='40' style ='text-align:left;font-size:12px;border-bottom-style: hidden;border-right-style: hidden;'><b> Vendor Name:</b> </td>\r\n      <td  colspan ='40' width='40'  style ='text-align:left;font-size:12px;border-bottom-style: hidden;'>{{doc.supplier}}</td>\r\n      <td colspan='25'   style ='text-align:left;font-size:12px;border-bottom-style: hidden;border-right-style: hidden;'><b>PO.No:</b> </td>\r\n      <td colspan='25'   style ='text-align:left;font-size:12px;border-bottom-style: hidden;'> {{doc.purchase_order_no}} </td>\r\n      <td colspan='25'   style ='text-align:left;font-size:12px;border-bottom-style: hidden;border-right-style: hidden;'><b>Invoice No: </b></td>\r\n      <td colspan='15'   style ='text-align:left;font-size:12px;border-bottom-style: hidden;'>{{doc.supplier_delivery_note}}</td>\r\n       </tr>\r\n        {% set po_date = frappe.db.get_value('Purchase Order',{'name':doc.purchase_order_no},'transaction_date') %}\r\n      <tr>\r\n       <td  colspan ='80' style ='text-align:left;font-size:15px'> <b>Address:</b></td>\r\n       <td colspan='50'   style ='text-align:date;font-size:15px'> Date: {{po_date}} </td>\r\n       <td colspan='50'   style ='text-align:date;font-size:15px'> Date:{{doc.posting_date}} </td>\r\n      </tr>\r\n      \r\n      <tr>\r\n        <td  colspan='10' style ='text-align:left;font-size:10px'> </td>\r\n        <td colspan='30'  style ='text-align:left;font-size:10px'> </td>\r\n        <td colspan='50'  style ='text-align:left;font-size:10px'> </td>\r\n        <td colspan='10'  style ='text-align:center;font-size:10px'> <b></b> </td>\r\n        <td colspan='30'  style ='text-align:center;font-size:10px'> <b>Quantity</b> </td>\r\n        <td colspan='20'  rowspan = \"2\" style ='text-align:center;font-size:10px'> <b> RIR Ref.No.</b> </td>\r\n        <td colspan='20'  style ='text-align:center;font-size:10px'> (For use by accounts) </td>\r\n      </tr>\r\n      \r\n      <tr>\r\n        <td  colspan='10' style ='text-align:left;font-size:10px'><b> SI.No.</b></td>\r\n        <td colspan='30'  style ='text-align:left;font-size:10px'><b> Item Code</b></td>\r\n        <td colspan='50'  style ='text-align:left;font-size:10px'><b>Description</b></td>\r\n        <td colspan='10'  style ='text-align:center;font-size:10px'><b>UOM</b> </td>\r\n        <td colspan='7'   style ='text-align:center;font-size:10px'> <b>Ordered</b></td>\r\n        <td colspan='7'   style ='text-align:center;font-size:10px'><b> Received</b></td>\r\n        <td colspan='8'   style ='text-align:center;font-size:10px'> <b>Accepted </b></td>\r\n        <td colspan='8'   style ='text-align:center;font-size:10px'><b> Rejected</b></td>\r\n        <td colspan='8'   style ='text-align:center;font-size:10px'><b>Unit Price</b></td>\r\n        <td colspan='8'   style ='text-align:center;font-size:10px'> <b>Total Price</b></td>\r\n       </tr>\r\n       {% for row in doc.items %}\r\n       <tr>\r\n        <td  colspan='10' style ='text-align:left;font-size:10px'> {{row.idx}}</td>\r\n        <td colspan='30'  style ='text-align:left;font-size:10px'> {{row.item_code}}</td>\r\n        <td colspan='50'  style ='text-align:left;font-size:10px'> {{row.item_name}}</td>\r\n        <td colspan='10'  style ='text-align:center;font-size:10px'>{{row.uom}}</td>\r\n        <td colspan='7'   style ='text-align:center;font-size:10px'>{{row.ordered}}</td>\r\n        <td colspan='7'   style ='text-align:center;font-size:10px'>{{row.received_qty}}</td>\r\n        <td colspan='8'   style ='text-align:center;font-size:10px'> {{row.qty}}</td>\r\n        <td colspan='8'   style ='text-align:center;font-size:10px'> {{row.rejected_qty}}</td>\r\n        <td colspan='17'  style ='text-align:center;font-size:10px'></td>\r\n        <td colspan='12'  style ='text-align:center;font-size:10px'> {{row.rate}}</td>\r\n        <td colspan='8'   style ='text-align:center;font-size:10px'>{{row.amount}}</td>\r\n       </tr>\r\n       \r\n       {% endfor%}\r\n      \r\n       <tr> \r\n         <td colspan='200'  style ='text-align:left;font-size:15px;\"height:100px;width:100px\"'>Remarks:. </td>\r\n        </tr>\r\n         \r\n         <tr>\r\n             <td colspan='50'  style ='text-align:left;font-size:12px'><b>Store Keeper:</b> </td>\r\n             <td colspan='50'  style ='text-align:left;font-size:12px'><b>Stores Head</b>:</td>\r\n             <td colspan='30'  style ='text-align:center;font-size:12px'><b>Q.C.Inspector/Concerned Sec.Rep.</b> </td>\r\n             <td colspan='30'  style ='text-align:left;font-size:12px'><b>Q.C.Head </b></td>\r\n             <td colspan='30'  style ='text-align:left;font-size:12px'><b>Accounts Dept.</b> </td>\r\n         </tr>\r\n         \r\n         <tr>\r\n             <td colspan='50'  style ='text-align:left;font-size:15px'>Date: </td> \r\n              <td colspan='50'  style ='text-align:left;font-size:15px'>Date: </td>\r\n               <td colspan='30'  style ='text-align:le